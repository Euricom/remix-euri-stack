{
  "name": "my-remix-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production SESSION_SECRET=s3cr3t node ./build/server/index.js",
    "test": "vitest",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:dev": "playwright test --ui",
    "typecheck": "tsc",
    "cleanup": "rimraf build server-build coverage",
    "docker:build": "docker build -f Dockerfile . -t $npm_package_name:latest -t $npm_package_name:$npm_package_version",
    "docker:run": "docker run --add-host=localhost:host-gateway --rm -it -p 3000:3000 --env-file .env --name $npm_package_name $npm_package_name:$npm_package_version",
    "docker:shell": "docker run --add-host=localhost:host-gateway --rm -it -p 3000:3000 --env-file .env --entrypoint /bin/sh $npm_package_name:$npm_package_version"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@remix-run/express": "2.12.1",
    "@remix-run/node": "^2.12.1",
    "@remix-run/react": "^2.12.1",
    "@remix-run/serve": "^2.12.1",
    "@sentry/profiling-node": "^8.32.0",
    "@sentry/remix": "^8.32.0",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "close-with-grace": "^2.1.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "hono": "^4.6.3",
    "isbot": "^4.1.0",
    "lucide-react": "^0.446.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-flat-routes": "^0.6.5",
    "remix-hono": "^0.0.16",
    "remix-utils": "^7.7.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.16.0",
    "@remix-run/dev": "^2.12.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitest/coverage-v8": "^2.1.1",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.0",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "msw": "^2.4.9",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.4",
    "tsx": "^4.19.1",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-env-only": "^3.0.3",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": "20"
  },
  "packageManager": "pnpm@9.11.0"
}
